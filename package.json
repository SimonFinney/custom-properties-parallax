  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.42",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "html-webpack-plugin": "^3.0.7",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
